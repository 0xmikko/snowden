{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "loose": true
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", {
      "loose": true
    }],
    "@babel/plugin-proposal-object-rest-spread",
    ["styled-jsx/babel", {
      "plugins": ["styled-jsx-plugin-sass"]
    }]
  ]
}
